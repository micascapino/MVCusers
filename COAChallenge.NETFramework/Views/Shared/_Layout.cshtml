<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - COA - Gestion de Usuarios</title>
    <link href="~/Content/Styles.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<div class="main">
    <body>
        <header>
            <div class="navbar">
                <h2>COA Challenge</h2>
                @Html.ActionLink("Gestion de usuarios", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                <a href="https://github.com/micascapino"><img src="~/Images/GitHubMark.png"></a>
            </div>
        </header>


        <div class="container body-content">
            @RenderBody()
            <hr />

        </div>
        <footer>
            <p>&copy; @DateTime.Now.Year - COA Challenge // Micaela Scapino</p>
        </footer>
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
    </body>

</div>

</html>
